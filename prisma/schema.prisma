generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model chipId {
  id      String   @id @default(uuid())
  name    String
  sensors sensor[]
}

model sensor {
  id     String  @id @default(uuid())
  chipId String
  status Boolean
  chip   chipId  @relation(fields: [chipId], references: [id])
}
